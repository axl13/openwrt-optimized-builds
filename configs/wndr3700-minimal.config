# Target Device
CONFIG_TARGET_ath79=y
CONFIG_TARGET_ath79_generic=y
CONFIG_TARGET_ath79_generic_DEVICE_netgear_wndr3700=y

# SFE Flow Offloading Kernel Modules for max performance
CONFIG_PACKAGE_kmod-fast-classifier=y
CONFIG_PACKAGE_kmod-shortcut-fe=y
CONFIG_PACKAGE_kmod-shortcut-fe-cm=y

# --- Minimal Configuration ---
# LuCI (Web Interface) is REMOVED to save space. Manage via SSH.
# CONFIG_PACKAGE_luci is not set
CONFIG_PACKAGE_luci-app-sfe=y

# IPv6 support is REMOVED.
# CONFIG_IPV6 is not set

# PPP (for PPPoE/PPPoA) is REMOVED. Only for DHCP/Static IP WAN.
# CONFIG_PACKAGE_ppp is not set

# USB Support is REMOVED.
# CONFIG_PACKAGE_kmod-usb-core is not set
